CREATE TABLE cca_oper.reconciliation_open (
register_id varchar(100) NOT NULL,
rec_id varchar(100) NOT NULL,
op_date date NOT NULL,
violation varchar(20) NOT NULL,
create_date timestamp NOT NULL,
modify_date timestamp NULL,
amount numeric(23, 2) NOT NULL,
product_factory_code varchar(50) NOT NULL,
binded_product_factory_code varchar(50) NULL,
business_service_id varchar(255) NULL,
account_id uuid NULL,
account varchar(50) NULL,
party_id varchar(255) NULL,
scenario varchar(100) NULL,
tb_code varchar(10) NULL,
depart_code varchar(20) NULL,
factory_tag varchar(100) NULL,
account_num varchar(50) NULL,
sharding varchar(10) NULL,
uniq_id uuid NOT NULL,
comm_instance_id varchar(255) NULL,
debt_product_id varchar(255) NULL,
debt_or_comm_code varchar(255) NULL,
service_code varchar(100) NULL,
initial_date timestamp NULL,
operation_user_name varchar(200) NULL,
transaction_code varchar(100) NULL,
scenario_template_code varchar(100) NULL,
CONSTRAINT reconciliation_open_pk PRIMARY KEY (register_id, rec_id, op_date)
)
PARTITION BY RANGE (op_date);
